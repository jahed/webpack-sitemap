{
  "name": "@jahed/webpack-sitemap",
  "version": "0.1.1",
  "description": "Sitemap Generator for Webpack",
  "author": "Jahed Ahmed",
  "license": "MIT",
  "files": [
    "dist",
    "templates"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc",
    "test": "echo 'No yests yet :('",
    "prepublishOnly": "yarn test && yarn run build",
    "postpublish": "git push --follow-tags",
    "docs": "bash -i ./docs.sh"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "dependencies": {
    "debug": "3.1.0",
    "handlebars": "4.0.11",
    "lodash": "4.17.10"
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/handlebars": "4.0.37",
    "@types/jest": "22.2.3",
    "@types/lodash": "4.14.108",
    "@types/node": "8.10.10",
    "@types/webpack": "4.1.4",
    "jest": "22.4.3",
    "rimraf": "2.6.2",
    "ts-jest": "22.4.4",
    "typedoc": "0.11.1",
    "typescript": "2.8.3"
  },
  "peerDependencies": {
    "webpack": ">=4"
  }
}
